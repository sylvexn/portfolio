{
  "name": "porto",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "backend:dev": "cd backend && python3 -m uvicorn main:app --reload --host 127.0.0.1 --port 3501",
    "backend:install": "cd backend && pip3 install -r requirements.txt",
    "backend:setup": "cd backend && python3 -m venv venv && source venv/bin/activate && pip install -r requirements.txt",
    "backend:setup:win": "powershell -Command \"cd backend; python -m venv venv; .\\venv\\Scripts\\Activate.ps1; python -m pip install -r requirements.txt\"",
    "backend:dev:win": "powershell -Command \"cd backend; .\\venv\\Scripts\\Activate.ps1; python -m uvicorn main:app --reload --host 127.0.0.1 --port 3501\"",
    "dev:all": "concurrently \"bun run backend:dev\" \"bun run dev\"",
    "setup": "bun install && bun run backend:setup",
    "setup:win": "bun install && bun run backend:setup:win",
    "start": "bun run preview",
    "backend:start": "cd backend && source venv/bin/activate && python3 -m uvicorn main:app --host 0.0.0.0 --port 3501"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-context-menu": "^2.2.15",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-hover-card": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.13",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.1.4",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@tailwindcss/line-clamp": "^0.4.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.5.0",
    "embla-carousel-react": "^8.6.0",
    "framer-motion": "^11.3.24",
    "lucide-react": "^0.469.0",
    "next-themes": "^0.4.6",
    "ogl": "^1.0.11",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "^7.56.4",
    "react-markdown": "^10.1.0",
    "sonner": "^2.0.3",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.25.28"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.0.0-beta.9",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "concurrently": "^9.1.0",
    "eslint": "^9.18.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "tailwindcss": "^4.0.0-beta.9",
    "typescript": "~5.7.2",
    "vite": "^6.0.7"
  }
}
